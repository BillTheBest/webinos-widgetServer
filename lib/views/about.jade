extends layout

block content
	div.infoPanel
		div  
			span= os 
			span  webinos runtime v1.0.0
		div 
			span node version 
			span= nodeVersion
			br
		div
			br
			span #{branding.appTitle} pzp configured to run on localhost:
			=pzpPort
			br
			span #{branding.appTitle} runtime configured to run on localhost:
			=runtimePort
		div
			br
			span project site 
			br
			a(target="_blank",href="http://#{branding.appUrl}") #{branding.appUrl}
			br
			br
			span developer portal 
			br
			a(target="_blank",href="http://developer.webinos.org") developer.webinos.org
		div
			br
			span copyright &copy; 2014 &middot; all rights reserved
